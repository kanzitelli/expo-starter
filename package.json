{
  "name": "expo-starter",
  "version": "6.4.0",
  "description": "Production-ready starter for Expo (React Native) App! Powered by cli-rn, React Navigation (v6), RN UI lib, Mobx, Reanimated 2, Dark Mode, Localization, and much more.",
  "author": "Batyr <dev@batyr.io> (https://github.com/kanzitelli)",
  "homepage": "https://github.com/kanzitelli/expo-starter",
  "scripts": {
    "start": "npx expo start",
    "start:dc": "npx expo start --dev-client",
    "start:prod": "npx expo start --no-dev --minify",
    "start:android": "npx expo start --android",
    "start:ios": "npx expo start --ios",
    "start:web": "npx expo start --web",
    "prebuild": "npx expo prebuild",
    "run:android": "npx expo run:android",
    "run:android:release": "npx expo run:android --variant release",
    "run:ios": "npx expo run:ios",
    "run:ios:release": "npx expo run:ios --configuration Release",
    "pub:web": "npx expo export:web && cd web-build && vercel --prod && cd ..",
    "pub:app:prod": "eas update --branch production",
    "pub:app:dev": "eas update --branch development",
    "pub:config": "eas update:configure",
    "pub:all": "yarn pub:app:dev && yarn pub:web",
    "format": "prettier --check ./src",
    "format:write": "prettier --write ./src",
    "release": "dotenv release-it",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@kanzitelli/if-component": "^0.3.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-picker/picker": "2.6.1",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/drawer": "^6.5.8",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@react-navigation/stack": "^6.3.0",
    "@shopify/flash-list": "1.6.3",
    "date-fns": "^2.29.3",
    "expo": "^50.0.14",
    "expo-application": "~5.8.3",
    "expo-dev-client": "~3.3.11",
    "expo-image": "~1.10.6",
    "expo-linking": "~6.2.2",
    "expo-localization": "~14.8.3",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.12",
    "formik": "^2.2.9",
    "i18n-js": "^4.2.3",
    "lodash": "^4.17.21",
    "mobx": "^6.8.0",
    "mobx-persist-store": "^1.1.3",
    "mobx-react": "^7.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-mmkv": "^2.7.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-ui-lib": "^7.2.1",
    "react-native-web": "~0.19.6",
    "rn-bounceable": "^1.2.0",
    "rn-navio": "^0.1.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/helper-skip-transparent-expression-wrappers": "^7.22.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-syntax-typescript": "^7.24.1",
    "@babel/plugin-transform-typescript": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/types": "^7.24.0",
    "@expo/config-plugins": "~7.8.0",
    "@expo/webpack-config": "~19.0.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/i18n-js": "^3.8.9",
    "@types/lodash": "^4.17.0",
    "@types/react": "~18.2.45",
    "@types/react-dom": "^18.2.22",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "release-it": "^17.1.1",
    "typescript": "^5.3.0"
  },
  "resolutions": {
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0"
  },
  "keywords": [
    "react",
    "react-native",
    "react-navigation",
    "react-native-starter",
    "react-native-template",
    "expo",
    "expo-modules",
    "expo-starter",
    "expo-template"
  ],
  "license": "MIT"
}
